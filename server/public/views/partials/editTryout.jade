div.container-fluid
  div
    h3(class="text-center") {{rev.review.val[0].title}}
    
    p Date {{ rev.review.val[0].dateString }} at {{ rev.review.val[0].time }}
    
    p Categories
    p(ng-repeat="category in rev.review.val[0].categories") {{ category.field }}
  
  button(class="btn btn-primary") Edit
  button(class="btn btn-primary") Save and Continue
  button(class="btn btn-primary") Continue
  
